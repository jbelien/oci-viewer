{
  "name": "@jbelien/oci-viewer",
  "description": "",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "oci": "run-p oci:*",
    "oci:combined": "cp node_modules/osm-community-index/dist/combined.min.geojson static/",
    "oci:feature": "rm -rf static/data/features/ && node build-feature.js",
    "oci:icons": "rm -rf static/icons/ && mkdir -p static/icons/ && cp node_modules/osm-community-index/dist/img/* static/icons/",
    "oci:resource": "rm -rf static/data/resources/ && node build-resource.js",
    "ol:css": "rm -f static/ol.css && cp node_modules/ol/ol.css static/",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "test": "eslint src/**/*.{js,svelte} build-*.js"
  },
  "dependencies": {
    "cldr": "^5.7.0",
    "compression": "^1.7.1",
    "ol": "^6.4.3",
    "osm-community-index": "^2.1.1",
    "polka": "next",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "colors": "^1.4.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.9",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.1",
    "sapper": "^0.28.3",
    "svelte": "^3.24.1"
  }
}
