{
  "name": "@jbelien/oci-viewer",
  "description": "",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "oci": "run-p oci:*",
    "oci:combined": "cp node_modules/osm-community-index/dist/completeFeatureCollection.min.json static/",
    "oci:feature": "rm -rf static/data/features/ && node scripts/build-feature.js",
    "oci:icons": "rm -rf static/icons/ && mkdir -p static/icons/ && cp node_modules/osm-community-index/dist/img/* static/icons/",
    "oci:resource": "rm -rf static/data/resources/ && node scripts/build-resource.js",
    "ol:css": "rm -f static/ol.css && cp node_modules/ol/ol.css static/",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "export:githubio": "GITHUBIO=true sapper export --legacy --basepath oci-viewer",
    "start": "node __sapper__/build",
    "lint": "eslint src/**/*.{js,svelte} scripts/*.js"
  },
  "dependencies": {
    "cldr": "^7.9.0",
    "compression": "^1.8.1",
    "ol": "^7.5.2",
    "osm-community-index": "^5.9.2",
    "polka": "next",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^5.0.7",
    "colors": "^1.4.0",
    "diacritics": "^1.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.79.2",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.3",
    "svelte": "^3.59.2"
  }
}
