{
  "name": "@jbelien/oci-viewer",
  "description": "",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "oci": "run-p oci:*",
    "oci:combined": "cp node_modules/osm-community-index/dist/completeFeatureCollection.min.json static/",
    "oci:feature": "rm -rf static/data/features/ && node build-feature.js",
    "oci:icons": "rm -rf static/icons/ && mkdir -p static/icons/ && cp node_modules/osm-community-index/dist/img/* static/icons/",
    "oci:resource": "rm -rf static/data/resources/ && node build-resource.js",
    "ol:css": "rm -f static/ol.css && cp node_modules/ol/ol.css static/",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "export:githubio": "GITHUBIO=true sapper export --legacy --basepath oci-viewer",
    "start": "node __sapper__/build",
    "lint": "eslint src/**/*.{js,svelte} build-*.js",
    "lint:fix": "eslint --fix src/**/*.{js,svelte} build-*.js",
    "deploy": "run-s oci ol:css export",
    "deploy:githubio": "run-s oci ol:css export:githubio"
  },
  "dependencies": {
    "cldr": "^6.1.0",
    "compression": "^1.7.4",
    "ol": "^6.5.0",
    "osm-community-index": "^3.1.2",
    "polka": "next",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.13.17",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "colors": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.45.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "svelte": "^3.37.0"
  }
}
